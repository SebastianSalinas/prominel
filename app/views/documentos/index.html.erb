<p id="notice"><%= notice %></p>

<h1>Listing Documentos</h1>

<table>
  <thead>
    <tr>
      <th>Cheque</th>
      <th>Chequera</th>
      <th>Valor</th>
      <th>Proveedor</th>
      <th>Fecha</th>
      <th>Oocc</th>
      <th>Factura</th>
      <th>Centrocosto</th>
      <th>Cobrado</th>
      <th>Nulo</th>
      <th>Nopago</th>
      <th>Plazo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @documentos.each do |documento| %>
      <tr>
        <td><%= documento.cheque %></td>
        <td><%= documento.chequera %></td>
        <td><%= documento.valor %></td>
        <td><%= documento.proveedor %></td>
        <td><%= documento.fecha %></td>
        <td><%= documento.oocc %></td>
        <td><%= documento.factura %></td>
        <td><%= documento.centrocosto %></td>
        <td><%= documento.cobrado %></td>
        <td><%= documento.nulo %></td>
        <td><%= documento.nopago %></td>
        <td><%= documento.plazo %></td>
        <td><%= link_to 'Show', documento %></td>
        <td><%= link_to 'Edit', edit_documento_path(documento) %></td>
        <td><%= link_to 'Destroy', documento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Documento', new_documento_path %>
